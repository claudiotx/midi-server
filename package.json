{
  "name": "enterprise-grade-nodejs-boilerplate",
  "version": "0.0.1",
  "description": "Typescript Enterprise Grade base boilerplate for Node.js apps",
  "keywords": [
    "enterprise",
    "professional",
    "engineering",
    "backend",
    "do-it-right"
  ],
  "homepage": "https://www.coderecipes.org",
  "main": "./dist/app.js",
  "bin": {
    "my-app": "./bin/app.js"
  },
  "scripts": {
    "start": "node ./dist/app.js",
    "start-pm2": "./scripts/launch-with-pm2.sh",
    "watch": "NODE_ENV=development LOG_LEVEL=debug concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\" \"npm run test-unit\" ",
    "serve": "nodemon ./dist/app.js",
    "serve-debug": "node debug ./dist/app.js",
    "dev": "npm run build && npm run watch",
    "watch-ts": "tsc -w",
    "build-ts": "tsc",
    "build": "tsc -v && npm run build-ts && npm run tslint",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "test": "jest -c .jest.config.js  --forceExit --coverage --watchAll",
    "test-unit": "jest -c .jest.config.unit.js --coverage --watch",
    "docs": "tsoa swagger",
    "prepush": "tsc -v && npm run tslint",
    "prestart": "node bin/prestart",
    "prewatch": "node bin/prestart"
  },
  "config": {
    "port": "7777"
  },
  "repository": {
    "type": "git",
    "url": "git+https://www.coderecipes.org"
  },
  "author": "Claudio Teixeira",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/owner/project/issues",
    "email": "contact@claudioteixeira.com"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.24",
    "@types/cors": "^2.8.4",
    "@types/lodash": "^4.14.116",
    "aws-sdk": "^2.347.0",
    "axios": "^0.18.0",
    "bella-scheduler": "^1.2.2",
    "bluebird": "^3.5.2",
    "btoa": "^1.2.1",
    "cors": "^2.8.4",
    "csv": "^4.0.0",
    "csv-stream": "^0.2.0",
    "csv-write-stream": "^2.0.0",
    "csvwriter": "^1.3.0",
    "detect-csv": "^1.1.0",
    "dotenv": "^5.0.0",
    "express": "^4.18.2",
    "express-brute": "^1.0.1",
    "express-rate-limit": "^3.3.2",
    "gnirts": "^1.1.5",
    "helmet": "^3.15.0",
    "json-to-csv-stream": "^1.1.0",
    "json2csv": "^4.2.1",
    "lodash": "^4.17.11",
    "md5": "^2.2.1",
    "mongoose": "^5.0.5",
    "morgan": "^1.9.0",
    "nanit": "^0.3.1",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-local": "^1.0.0",
    "s3-streamlogger": "^1.4.0",
    "simple-get": "^3.0.3",
    "smart-buffer": "^4.0.1",
    "socket.io": "^2.0.4",
    "toobusy-js": "^0.5.1",
    "ts-node": "^5.0.1",
    "uuid": "^3.2.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/btoa": "^1.2.0",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.11.1",
    "@types/express-brute": "^0.0.37",
    "@types/express-rate-limit": "^3.3.0",
    "@types/helmet": "^0.0.42",
    "@types/jest": "^22.1.2",
    "@types/json2csv": "^4.2.0",
    "@types/md5": "^2.1.33",
    "@types/mongodb": "^3.0.5",
    "@types/mongoose": "^5.0.2",
    "@types/morgan": "^1.7.35",
    "@types/passport": "^1.0.0",
    "@types/passport-facebook": "^2.1.8",
    "@types/passport-local": "^1.0.33",
    "@types/semver": "^5.5.0",
    "@types/socket.io": "^1.4.31",
    "@types/supertest": "^2.0.4",
    "@types/toobusy-js": "^0.5.2",
    "@types/uuid": "^3.4.3",
    "concurrently": "^3.5.1",
    "husky": "^0.14.3",
    "jest": "^22.3.0",
    "nodemon": "^1.14.12",
    "semver": "^5.6.0",
    "supertest": "^3.0.0",
    "ts-jest": "^22.0.4",
    "tslint": "^5.9.1",
    "tsoa": "^2.1.4",
    "typescript": "^3.3.3"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/test/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 5.0.0"
  },
  "os": [
    "darwin",
    "linux"
  ]
}
